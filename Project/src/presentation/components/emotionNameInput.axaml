<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        x:Class="Project.application.components.emotionNameInput"
        Width="400" Height="150"
        WindowStartupLocation="CenterOwner"
        CanResize="False"
        Title="Nombre de la emoción">

    <StackPanel Margin="20" Spacing="10">
        <TextBlock Text="Ingrese el nombre de la nueva emoción:"/>
        <TextBox x:Name="NameTextBox" />
		<StackPanel Orientation="Horizontal" HorizontalAlignment="Right" Spacing="10">
            <Button Content="Cancelar" Click="Cancel_Click"/>
            <Button Content="Aceptar" Click="Accept_Click"/>
        </StackPanel>
    </StackPanel>
</Window>
